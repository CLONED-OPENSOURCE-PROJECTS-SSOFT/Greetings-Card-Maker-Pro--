	<LinearLayout
	    android:id="@+id/storage_settings_editCCSettings"
		android:layout_width="match_parent"
		android:layout_height="@dimen/adobe_storage_setting_item_height"
		android:background="@drawable/storage_settings_item_divider"
		android:gravity="center_vertical"
		android:orientation="vertical" >

		<com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/storage_settings_edit_cc_settings"
			android:layout_gravity="center_vertical"
			android:textColor="@color/asset_browser_dark_text"
			android:textSize="@dimen/adobe_storage_setting_text_size" />
	</LinearLayout>
	

    <LinearLayout
        android:id="@+id/storage_settings_switchAccounts"
		android:layout_width="match_parent"
		android:layout_height="@dimen/adobe_storage_setting_item_height"
		android:background="@drawable/storage_settings_item_divider"
		android:gravity="center_vertical"
		android:orientation="vertical" >

		<com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
			
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/storage_settings_switch_accounts"
			android:layout_gravity="center_vertical"
			android:textColor="@color/asset_browser_dark_text"
			android:textSize="@dimen/adobe_storage_setting_text_size" />
	</LinearLayout>
    
    
</LinearLayout>
