<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:minHeight="170dp"
    android:minWidth="220dp"
    android:orientation="vertical"
    android:background="@color/creative_sdk_actionbar_background_color">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="130dp">
    <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:id="@+id/alert_dialog_box_title_text"
        android:text="@string/COLLABORATION_DELETE_MESSAGE"
        android:singleLine="false"
        android:layout_alignParentTop="true"
        android:maxLines="2"
        android:textSize="19sp"
        android:layout_height="wrap_content" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/assetview_dialog_fragment_layout_height">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true">
            <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
                android:id="@+id/alert_dialog_box_negative_button"
                android:layout_height="match_parent"
                android:layout_width="wrap_content"
                android:textColor="@color/creative_sdk_blue_color"
                android:textSize="18sp"
                android:text="@string/COLLABORATOR_DELETE_NEGATIVE_BUTTON"
                android:layout_weight="1"
                />
            <RelativeLayout
                android:layout_width="25dp"
                android:layout_height="match_parent"></RelativeLayout>
            <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
                android:id="@+id/alert_dialog_box_positive_button"
                android:layout_height="match_parent"
                android:layout_width="wrap_content"
                android:text="@string/COLLABORATOR_DELETE_POSITIVE_BUTTON"
                android:textColor="@color/creative_sdk_blue_color"
                android:textSize="18sp"
                android:layout_weight="1"
                />
        </LinearLayout>
    </RelativeLayout>

</LinearLayout><!-- From: file:/Users/bharath/GIT/CreativeSDKRoot/creativesdk/Foundation/CreativeSDKFoundationAssetUX/res/layout/adobe_alert_dialog_view.xml --><!-- From: file:/C:/Users/jagota/CreativeSDKSampleApp/build/intermediates/exploded-aar/com.adobe.creativesdk.foundation/assetux/0.7.329/res/layout/adobe_alert_dialog_view.xml -->