<?xml version="1.0" encoding="utf-8"?>
<com.adobe.creativesdk.foundation.internal.storage.controllers.AdobeAssetViewSectionalListFolderViewCell
     xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/assetbrowser_list_view_image_left_padding"
    android:id="@+id/storage_assetlist_foldercell_root"  >
    
      
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        
        <include layout="@layout/adobe_storage_assetslist_header" />
        
        <include layout="@layout/adobe_storage_assetslist_divider" />
        
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" 
            			android:layout_height="@dimen/assetbrowser_list_view_item_height"
 
            			>
	                
	        <ImageView android:id="@+id/storage_assetlist_foldercell_image" 
	        	    android:layout_width="@dimen/assetbrowser_list_view_image_dimension" 
	        	    android:layout_height="@dimen/assetbrowser_list_view_image_dimension"
	        	    android:src="@drawable/collection_folder"
	        	    android:gravity="center"
	        	    android:layout_marginTop="@dimen/assetbrowser_list_view_image_top_padding"/>
	        	
	     
	       <com.adobe.creativesdk.foundation.internal.utils.CreativeSDKTextView
                android:id="@+id/storage_assetlist_foldercell_title"
                android:layout_width="@dimen/assetbrowser_list_view_folder_text_layout_width"
                android:layout_marginLeft="@dimen/assetbrowser_list_view_text_left_padding"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:ellipsize="middle"
                android:gravity="center_vertical"
                android:singleLine="true" 
                android:textSize="@dimen/assetbrowser_list_view_text_size"
                android:background="@color/creative_sdk_transparent_color"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginLeft="@dimen/assetbrowser_list_view_folder_forward_left_padding"
                android:layout_marginRight="@dimen/assetbrowser_list_view_folder_forward_right_padding"
             
                android:src="@drawable/folder_forward_icon" 
                android:background="@color/creative_sdk_transparent_color"/>
	        
	      
	        
	        
	        
  		 </LinearLayout>
    
    </LinearLayout>
    
</com.adobe.creativesdk.foundation.internal.storage.controllers.AdobeAssetViewSectionalListFolderViewCell>
